    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/platform-ws": "^7.6.18",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^7.1.5",
