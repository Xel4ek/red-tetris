{
  "name": "red-tetris",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "dev": "concurrently --kill-others \"nx run api:serve\" \"nx run client:serve:development\"",
    "start:dev:db": " ./apps/api/src/scripts/start-db.sh",
    "start": "ng serve",
    "build": "nx run api:build:production && nx run client:build:production",
    "test": "nx run-many --all --target=test --detectOpenHandles",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "typeorm": "ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js src/main.ts",
    "migration": "typeorm migration:run",
    "migration:create": "typeorm migration:create -n",
    "migration:revert": "typeorm migration:revert"
  },
  "private": true,
  "dependencies": {
